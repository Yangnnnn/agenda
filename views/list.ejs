
<%- include('head'); -%>

  <div class="box" id="heading">
    <h1><%= listTitle %></h1>
  </div>


<div class="box">
  <%for(var i=0;i<Lists.length;i++){ %>
    <div class="item">
      <input type="checkbox">
      <p><%= Lists[i] %></p>
    </div>
    <% } %>


    <form action= "/" method="post">
      <input type="text" name="new" placeholder="New items">
      <button type="submit" name="button" value = <%= listTitle %>>+</button>
    </form>
</div>
